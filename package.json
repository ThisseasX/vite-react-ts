{
  "name": "vite-react-ts",
  "version": "1.0.0",
  "description": "A React project with Vite, TypeScript, Redux, Redux-Observable, and RxJS.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "vite --open",
    "build": "vite build",
    "types": "tsc",
    "lint": "eslint --fix src",
    "prettier": "prettier --write src",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "ThisseasX",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux-observable": "3.0.0-rc.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "msw": "^2.3.1",
    "prettier": "^3.3.0",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.12"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
